<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!--waveView 波浪-->
    <declare-styleable name="waveView">
        <attr name="waveColor" format="color|reference"/>
        <!--周期因子 W-->
        <attr name="period_factor" format="integer"/>
        <!--振幅因子 A-->
        <attr name="amplitude_factor" format="integer"/>
    </declare-styleable>


    <!--三角形-->
    <declare-styleable name="TriangleView">
        <!--颜色 -->
        <attr name="ViewColor" format="color|reference"/>
        <!--浮标大小 -->
        <attr name="ViewCursorSize" format="dimension"/>
    </declare-styleable>


    <!--彩色环形进度条-->
    <declare-styleable name="ColorProgressBar">
        <attr name="back_color" format="color"/>
        <attr name="front_color1" format="color"/>
        <attr name="front_color2" format="color"/>
        <attr name="front_color3" format="color"/>
        <attr name="back_width" format="dimension"/>
        <attr name="front_width" format="dimension"/>
        <attr name="pointer_width" format="dimension"/>
        <attr name="total_engle" format="integer"/>
        <attr name="current_value" format="float"/>
        <attr name="max_value" format="float"/>
        <attr name="show_point_process" format="boolean" />
        <attr name="diameter_value" format="integer"/>
    </declare-styleable>


</resources>
